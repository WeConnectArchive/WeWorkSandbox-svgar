{
  "name": "svgar",
  "version": "0.1.3",
  "description": "svg, but a little sweeter",
  "main": "dist/index.js",
  "module": "commonjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
