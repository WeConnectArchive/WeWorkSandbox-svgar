{
  "name": "svgar",
  "version": "0.4.3",
  "description": "svg, but a little sweeter",
  "main": "dist/index.js",
  "module": "commonjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/*.d.ts",
      "index.ts",
      "dist/**",
      "coverage/**"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeWorkSandbox/svgar.git"
  },
  "keywords": [
    "svg"
  ],
  "author": "Chuck",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/WeConnect/svgar/issues"
  },
  "homepage": "https://github.com/WeConnect/svgar#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "ts-node": "6.2.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {}
}
